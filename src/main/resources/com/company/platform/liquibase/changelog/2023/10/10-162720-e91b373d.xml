<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="platform">
        <createTable tableName="TEST_SEN">
            <column name="ID" type="VARCHAR(255)">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_TEST_SEN"/>
            </column>
            <column name="SN" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="RECORD_TIME" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="TEMP" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="HUMIDITY" type="FLOAT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>